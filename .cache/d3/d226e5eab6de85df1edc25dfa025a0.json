kground-image: url('../img/menu.png');\n    background-repeat: no-repeat;\n    background-size: contain;\n    background-position: center;\n    padding: 1rem;\n    border: 0;\n    background-color: rgba(0, 0, 0, 0);\n    position: absolute;\n    top: 0;\n    bottom: 0;\n    left: 0.5rem;\n}\n\n.nav-divider {\n    z-index: 2;\n    transition: width 1.5s ease 0s;\n    width: 100%;\n    height: 2px;\n    background: repeating-linear-gradient(to right, rgba(53, 65, 71, 0.3) 0px, rgba(53, 65, 71, 0.3) 6px, transparent 6px, transparent 9px);\n}\n\np {\n    max-width: 640px;\n    margin: 1rem auto;\n}\n\n/* MAIN */\n\n.wrapper {\n    padding: 1rem;\n}\n\nh1,\nh2,\nh3 {\n    font-family: 'Roboto Slab', serif;\n    color: #323C4C;\n}\n\n.hero__title {\n    text-align: center;\n    font-size: 2.5rem;\n}\n\n@media (min-width: 500px) {\n    .hero__title {\n        font-size: 3.5rem;\n    }\n}\n\n\na.btn-style {\n    background-color: #EAC455;\n    padding: 1rem;\n    border-radius: 5px;\n    font-size: 1.25rem;\n    margin: auto;\n    display: block;\n    width: 15rem;\n    text-align: center;\n    color: #1E283A;\n    text-decoration: none;\n}\n\n.page-links {\n    margin: auto;\n    text-align: center;\n}\n\n.page-links a {\n    color: #323C4C;\n    display: inline-block;\n    margin: 2.5rem 1rem;\n    font-size: 1.125rem;\n}\n\n/* INTRO */\n\n\n\n/* INSTAGRAM */\n#instagram-feed {\n    min-height: 30vh;\n    margin-top: 10vh;\n    background-color: #7893C4;\n}\n\n/* WHY MICHAEL */\n#why-michael {\n    display: block;\n    margin: auto;\n    max-width: 80vw;\n    margin-top: 10vh;\n}\n\n#why-michael h3 {\n    width: 50%;\n    text-align: center;\n    margin: auto;\n}\n\n@media (min-width: 500px) {\n    .why-choose-container {\n        display: flex;\n        flex-direction: row;\n        flex-wrap: nowrap;\n        width: 100%;\n        align-items: center;\n        justify-content: space-evenly;\n    }\n}\n\n#why-michael .img-container {\n    flex: 1;\n}\n\nimg {\n    max-width: 250px;\n}\n\n#why-michael ul {\n    list-style: none;\n    padding-left: 0;\n    padding-right: 1.5rem;\n    max-width: 640px;\n    flex: 2;\n}\n\n#why-michael li {\n    margin: 1rem auto;\n}\n\nfooter {\n    background-color: #fff;\n    width: 100vw;\n    padding-top: 1rem;\n    bottom: 0;\n}"},"lineCount":null}},"error":null,"hash":"9fcd65f264029fbedc00079f9d9a0595","cacheData":{"env":{}}}